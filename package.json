{
  "name": "@ponteggio",
  "version": "0.0.1",
  "description": "",
  "author": "Kamil Selwa",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --detectOpenHandles --logHeapUsage --runInBand",
    "test:all": "LOGLEVEL=silent node --expose-gc ./node_modules/jest/bin/jest -i --detectOpenHandles --forceExit --logHeapUsage --config ./test/jest-all.json",
    "test:watch": "LOGLEVEL=error jest --watch --runInBand",
    "test:cov": "npm run test:all -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "3.0.0",
    "@nestjs/common": "10.2.7",
    "@nestjs/config": "3.1.1",
    "@nestjs/core": "^10.2.7",
    "@nestjs/cqrs": "10.2.6",
    "@nestjs/jwt": "10.1.1",
    "@nestjs/mongoose": "10.0.1",
    "@nestjs/passport": "10.0.2",
    "@nestjs/platform-express": "10.2.7",
    "@nestjs/schedule": "^4.0.0",
    "@nestjs/swagger": "7.1.14",
    "@nestjs/websockets": "10.2.7",
    "@opentelemetry/api": "1.6.0",
    "ajv": "8.12.0",
    "ajv-formats": "2.1.1",
    "aws-sdk": "2.1482.0",
    "axios": "^1.6.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "email-templates": "11.1.1",
    "fast-xml-parser": "4.3.2",
    "form-data": "^4.0.0",
    "formdata-node": "^6.0.3",
    "jwks-rsa": "3.1.0",
    "lodash": "4.17.21",
    "markdown": "~0.5.0",
    "mongodb": "5.9.0",
    "mongoose": "7.6.3",
    "nanoid": "5.0.2",
    "nestjs-i18n": "10.3.7",
    "nestjs-pino": "3.5.0",
    "node-forge": "^1.3.1",
    "nodemailer": "6.9.7",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "pino": "8.16.1",
    "pino-http": "8.5.0",
    "pino-noir": "2.2.1",
    "pino-pretty": "^10.2.3",
    "ramda": "0.29.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "10.2.0",
    "@nestjs/schematics": "10.0.2",
    "@nestjs/testing": "10.2.7",
    "@types/cron": "^2.0.1",
    "@types/express": "4.17.20",
    "@types/jest": "29.5.6",
    "@types/node": "20.8.9",
    "@types/supertest": "2.0.15",
    "@types/temp-write": "^4.0.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "jest": "29.7.0",
    "json-schema-to-ts": "^2.9.2",
    "jsonwebtoken": "9.0.2",
    "prettier": "3.0.3",
    "source-map-support": "0.5.21",
    "supertest": "6.3.3",
    "temp-write": "5.0.0",
    "ts-jest": "29.1.1",
    "ts-loader": "9.5.0",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.2.2",
    "webpack": "5.89.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
