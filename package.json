{
  "name": "@wizkey/pontegg.io",
  "version": "0.0.1",
  "description": "",
  "author": "Kamil Selwa",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --project node_modules/@wizkey/bellerofonte-shared/ && tsc --project node_modules/@wizkey/bellerocontracts-sdk/ && nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --forceExit --detectOpenHandles --logHeapUsage --runInBand",
    "test:all": "LOGLEVEL=silent node --expose-gc ./node_modules/jest/bin/jest -i --detectOpenHandles --forceExit --logHeapUsage --config ./test/jest-all.json",
    "test:watch": "LOGLEVEL=error jest --watch --runInBand",
    "test:cov": "npm run test:all -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@nestjs/axios": "1.0.0",
    "@nestjs/common": "9.2.1",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/cqrs": "9.0.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mongoose": "9.2.1",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/swagger": "6.1.3",
    "@nestjs/websockets": "9.2.1",
    "@opentelemetry/api": "1.3.0",
    "@typegoose/typegoose": "9.13.1",
    "ajv": "8.11.2",
    "ajv-formats": "2.1.1",
    "aws-sdk": "2.1264.0",
    "axios": "^1.2.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "email-templates": "10.0.1",
    "ethers": "^5.7.2",
    "fast-xml-parser": "4.0.12",
    "form-data": "^4.0.0",
    "formdata-node": "^5.0.0",
    "jwks-rsa": "3.0.0",
